@code {
    [Parameter]
    public string? ReturnUrl { get; set; }
    protected override void OnInitialized()
    {
        if(ReturnUrl != null)
        {
            ReturnUrl = ReturnUrl;
            _navManager.NavigateTo($"users/Login?returnUrl={ReturnUrl}", forceLoad: true);
        }  
        else _navManager.NavigateTo($"users/Login", forceLoad: true);

    }
}
